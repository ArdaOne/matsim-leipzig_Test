        <length meter="15.0"/>
        <width meter="1.0"/>
        <passengerCarEquivalents pce="3.5"/>
        <networkMode networkMode="car"/>
        <flowEfficiencyFactor factor="1.0"/>
    </vehicleType>
	
	<vehicleType id="bike">
		<attributes>
            <attribute name="accessTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
            <attribute name="egressTimeInSecondsPerPerson" class="java.lang.Double">1.0</attribute>
        </attributes>
		<length meter="2.0"/>
		<width meter="1.0"/>
		<maximumVelocity meterPerSecond="6.82"/>
		<!--  from MiD average bike speed in berlin * 2, so on most links they go 12km/h and on bike highways close to 24km/h  -->
		<passengerCarEquivalents pce="0.1"/>
	</vehicleType>
    
    <vehicleType id="conventional_vehicle">
